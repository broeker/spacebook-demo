---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - page
    - pages
    - tagList
  addAllPagesToCollections: true
layout: layouts/page.njk
eleventyComputed:
  title: Pages tagged “{{ tag }}”
permalink: /tags/{{ tag | slug }}/
---
{%  if config.enableTags %}
{% set postslist = collections[ tag ] %}
{% include "templates/pageList.njk" %}
<p>See <a href="{{ '/tags/' | url }}">all tags</a>.</p>
{%  else  %}
  <p>Tagging not enabled.</p>
{%  endif %}